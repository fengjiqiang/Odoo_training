        <xpath expr="script[last()]" position="after">
            <script type="text/javascript" src="/web_tree_view/static/src/lib/zTree_v3/js/jquery.ztree.all.js"></script>
            <script type="text/javascript" src="/web_tree_view/static/src/js/treeview.js"/>
        </xpath>
    </template>
</odoo>
